            <table>
                    <tr>
                        <td>Name</td>
                        <td>{{data.name}}</td>
                    </tr>
                    <tr>
                        <td>Email</td>
                        <td>{{data.email}}</td>
                    </tr>
                    {{#each data.answers}}
                        <tr>
                            <td>{{this.question}}</td>
                            <td>{{this.answer}}</td>
                        </tr>
                    {{/each}}
                </table>
                
                <style>
                    table {
                        font-family: arial, sans-serif;
                        border-collapse: collapse;
                        width: 100%;
                    }
                
                    td, th {
                        border: 1px solid #dddddd;
                        text-align: left;
                        padding: 8px;
                    }
                
                    tr:nth-child(even) {
                        background-color: #dddddd;
                    }
                </style>